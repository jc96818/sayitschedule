{
  "name": "sayitschedule-backend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx src/db/seed.ts",
    "db:import-help": "tsx src/db/importHelpContent.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.712.0",
    "@aws-sdk/client-ses": "^3.958.0",
    "@aws-sdk/client-transcribe-streaming": "^3.712.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@fastify/websocket": "^10.0.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/pdfkit": "^0.17.4",
    "bcrypt": "^5.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^4.26.2",
    "nanoid": "^5.0.6",
    "openai": "^6.15.0",
    "otplib": "^12.0.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "qrcode": "^1.5.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.30",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vitest": "^1.6.0"
  }
}
